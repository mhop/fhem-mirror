    </reg>
    <reg name="Binary output 7" id="19">
      <endpoint name="Binary 7" type="bin" dir="out"/>
    </reg>
    <reg name="PWM output 0" id="20">
      <endpoint name="PWM output 0" type="num" dir="out"/>
