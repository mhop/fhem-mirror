    <reg name="Binary output 4" id="16">
      <endpoint name="Binary 4" type="bin" dir="out"/>
    </reg>
    <reg name="Binary output 5" id="17">
      <endpoint name="Binary 5" type="bin" dir="out"/>
    </reg>
    <reg name="Binary output 6" id="18">
